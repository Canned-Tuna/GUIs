-- Scripts:

local function UTEPR_fake_script() -- W.LocalScript 
	local script = Instance.new('LocalScript', W)

	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(inputObject)
		if inputObject.KeyCode == Enum.KeyCode.W then
			script.Parent.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
		end
	end)
	
	UserInputService.InputEnded:Connect(function(inputObject)
		if inputObject.KeyCode == Enum.KeyCode.W then
			script.Parent.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		end
	end)
end
coroutine.wrap(UTEPR_fake_script)()
