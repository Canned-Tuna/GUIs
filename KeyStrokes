local function TVLXBMY_fake_script() -- TopBar.LocalScript 
	local script = Instance.new('LocalScript', TopBar)

	local mouse = game.Players.LocalPlayer:GetMouse()
	
	spawn(function()
		local clicks = {}
		local mouse = game.Players.LocalPlayer:GetMouse()
		mouse.Button1Up:Connect(function()
			table.insert(clicks, tick())
		end)
		while wait() do
			for i,v in next,clicks do
				if v+1 < tick() then
					table.remove(clicks, i)
				end
			end
			script.Parent.Parent.LCPS.count.Text = tostring(#clicks)
		end
	end)
	
	spawn(function()
		local clicks = {}
		local mouse = game.Players.LocalPlayer:GetMouse()
		mouse.Button2Up:Connect(function()
			table.insert(clicks, tick())
		end)
		while wait() do
			for i,v in next,clicks do
				if v+1 < tick() then
					table.remove(clicks, i)
				end
			end
			script.Parent.Parent.RCPS.count.Text = tostring(#clicks)
		end
	end)
	
end
coroutine.wrap(TVLXBMY_fake_script)()
local function ABZTNBJ_fake_script() -- Holder.Close 
	local script = Instance.new('LocalScript', Holder)

	local UserInputService = game:GetService("UserInputService")
	
	UserInputService.InputBegan:Connect(function(A)
		if A.KeyCode == Enum.KeyCode.Insert then
			if script.Parent.Visible == true then
				script.Parent.Visible = false
				else script.Parent.Visible = true
			end
		end
	end)
	
	UserInputService.InputBegan:Connect(function(B)
		if B.KeyCode == Enum.KeyCode.Delete then
			script.Parent:Remove()
		end
	end)
end
coroutine.wrap(ABZTNBJ_fake_script)() 
