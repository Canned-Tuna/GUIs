local function MYWQF_fake_script() -- LMB.LocalScript 
	local script = Instance.new('LocalScript', LMB)

	local mouse = game.Players.LocalPlayer:GetMouse()
	
	mouse.Button1Down:Connect(function()
		script.Parent.BackgroundColor3 = Color3.fromRGB(80,  80, 80)
	end)
	
	mouse.Button1Up:Connect(function()
		script.Parent.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	end)
end
coroutine.wrap(MYWQF_fake_script)()
